{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "experimentalDecorators": true, // 开启装饰器语法
    "strictNullChecks": false, // 为true是定义class必须给属性赋值
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "sweety-form": ["src", "example"],
      "sweety-form/*": ["src/*", "*", "example/*"]
    }
  },
  "include": [".dumirc.ts", "src/**/*", "docs/test.tsx", "example/**/*"]
}
